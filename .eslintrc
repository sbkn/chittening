{
  "parserOptions": {
	"sourceType": "module",
	"ecmaFeatures": {
	  "jsx": true
	}
  },
  "env": {
	"browser": true,
	"node": true
  },
  "rules": {
	"quotes": [
	  2,
	  "double"
	],
	"strict": [
	  2,
	  "never"
	],
	"valid-jsdoc": 1,
	"eqeqeq": 2,
	"no-alert": 2,
	"no-eval": 2,
	"react/jsx-uses-react": 1,
	"react/jsx-uses-vars": 1,
	"react/react-in-jsx-scope": 1
  },
  "plugins": [
	"react"
  ],
  "extends": [
	"eslint:recommended",
	"plugin:react/recommended",
	"react-app"
  ]
}
